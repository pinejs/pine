{
  "name": "pine.js",
  "version": "1.0.3",
  "description": "A restful framework inspired by loopback",
  "main": "index.js",
  "scripts": {
    "build": "babel src/ --out-dir lib",
    "lint": "eslint src test *.js",
    "test": "ava test/**/*.spec.js",
    "test-cov": "nyc ava test/**/*.test.js && nyc report --reporter=lcov",
    "prepublish": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frankliu/pine.js.git"
  },
  "keywords": [
    "restful",
    "framework",
    "loopback"
  ],
  "author": "Frank Liu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frankliu/pine.js/issues"
  },
  "homepage": "https://github.com/frankliu/pine.js#readme",
  "ava": {
    "require": "babel-register",
    "babel": {
      "babelrc": true
    }
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ]
  },
  "dependencies": {
    "boom": "^4.3.1",
    "debug": "^2.6.3",
    "escape-html": "^1.0.3",
    "globby": "^6.1.0",
    "helmet": "^3.5.0",
    "is-type-of": "^1.0.0",
    "kcors": "^1.3.2",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^3.2.1",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-graphql": "^0.7.0",
    "koa-logger": "^2.0.1",
    "koa-oai-router": "^1.0.12",
    "koa-ratelimit": "^4.0.0",
    "koa-redis": "^2.1.3",
    "koa-response-time": "^2.0.0",
    "koa-rest-router": "^1.0.1",
    "koa-roles": "^1.0.3",
    "koa-router": "^7.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-uuid": "^1.4.8",
    "reflect-metadata": "^0.1.10",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.5.1",
    "eslint": "^3.18.0",
    "nyc": "^10.2.0"
  }
}
